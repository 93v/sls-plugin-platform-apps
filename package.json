  "homepage": "https://github.com/93v/sls-plugin-platform-apps#readme",
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "@types/serverless": "^1.18.2",
    "eslint": "^6.0.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.0",
